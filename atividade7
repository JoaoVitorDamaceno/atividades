	funcao inicio()
	{
		inteiro vetor[4] = {5,7,5,9}

		 para(inteiro i = 0; i < 4; i++)
		 {
		 	escreva("\n" + vetor[i])
		 	se(i == 3)
		 	{
		 		inteiro media = (vetor[0] + vetor[1] + vetor[2] + vetor[3]) / i
		 		escreva("\n" + "-------------------" + "\n" + media)
		 	}
		 }
	}
}

