programa
{
	
	funcao inicio()
	{
		inteiro vetor[10]

		para(inteiro i = 0; i < 10; i++)
		{
			escreva("\n"+"Insira um numero: ")
			leia(vetor[i])
		}

		escreva("-------------------")

		para(inteiro i = 0; i < 10; i++)
		{
			se(vetor[i] % 2 == 0)
			{
			escreva("\n" + vetor[i] + " é par e está no vetor " + i)
			}
		}
	}
}

\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
6-
\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\

programa
{
	
	inclua biblioteca Util
	
	funcao inicio()
	{
		inteiro vetor[30]
		inteiro chave
		inteiro vezes = 0

		para(inteiro i = 0; i < 30; i++)
		{
			vetor[i] = sorteia(1, 15)
		}

		escreva("Digite a chave de 1 a 15: ")
		leia(chave)

		escreva("\n" + "As posições que sua chave se encontra são:")
		
		para(inteiro i = 0; i < 30; i++)
		{
			se (vetor[i] == chave)
			{
				escreva("\n" + i)
				vezes++
			}

		}

		escreva("\nO numero de vezes que sua chave apareceu foi: \n" + vezes)
	}
}

\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
7-
\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\

programa
{
	
inclua biblioteca Util --> u
	
	funcao inicio()
	{
		inteiro listadeImpares[5]
		inteiro listadePares[5]
		inteiro listaIntercalados[10]
		inteiro numeroElementos
		logico parOuImpar = verdadeiro
		inteiro parRodada = 0
		inteiro imparRodada = 0

		//pedindo numeros impares
		para(inteiro i = 0; i < 5; i++)
		{
			escreva("Escreva o " + (i + 1) + " numero impar: ")
			leia(listadeImpares[i])
			
			enquanto(listadeImpares[i] % 2 == 0)
			{
				escreva("Numero invalido tente novamente: ")
				leia(listadeImpares[i])
			}
			
		}

		//pedindo numeros pares
		para(inteiro i = 0; i < 5; i++)
		{
			escreva("Escreva o " + (i + 1) + " numero par: ")
			leia(listadePares[i])
			
			enquanto(listadePares[i] % 2 != 0)
			{
				escreva("Numero invalido tente novamente: ")
				leia(listadePares[i])
			}
			
		}

		escreva("\n"+ "-------------" + "\n")

		//mostrando valores intercalados
		para(inteiro i = 0; i < 10; i++)
		{
			
			se(parOuImpar == falso)
			{
				listaIntercalados[i] = listadePares[parRodada]
				parRodada++
				parOuImpar = verdadeiro
				escreva("\n" + listaIntercalados[i] + " do vetor dos pares")
			}

			senao
			{
				listaIntercalados[i] = listadeImpares[imparRodada]
				imparRodada++
				parOuImpar = falso
				escreva("\n" + listaIntercalados[i] + " do vetor dos impares")
			}

		}
	}
}

\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
8-
\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\

programa
{
	
	funcao inicio()
	{
		inteiro linha = 0
		inteiro coluna = 0
		inteiro numeros[3][3] = {
			
			{1,3,5},
			{7,9,11},
			{13,15,17}

		}
			
		enquanto(coluna != 2)
		{	
			enquanto (linha < 3)
			{
				escreva(" " + numeros[coluna][linha])
				linha++	
				se(linha == 2 e coluna != 2)
				{
					escreva(" " + numeros[coluna][linha])
					linha = 0
					coluna++
					escreva("\n")
				}
			}	

		}		
		
	}
}

\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
9-
\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\

programa
{
	
	funcao inicio()
	{
		inteiro linha = 0
		inteiro coluna = 0
		inteiro resultado = 0
		inteiro numeros[3][3] = {
			
			{1,3,5},
			{7,9,11},
			{13,15,17}

		}
			
		enquanto(coluna != 2)
		{	
			enquanto (linha < 3)
			{
				resultado += numeros[coluna][linha]
				linha++	
				se(linha == 2 e coluna != 2)
				{
					resultado += numeros[coluna][linha]
					linha = 0
					coluna++
				}
			}	

		}

		escreva("O resultado da soma da matriz foi: " + resultado)
		
	}
}

\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
10-
\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\

programa
{
	
	funcao inicio()
	{
		inteiro coluna = 0
		inteiro linha = 0
		logico calculo_pares = falso
		inteiro numeros[5][3]

		enquanto(coluna != 4)
		{	
			enquanto (linha < 3)
			{
				escreva("Digite um numero para a linha " + (linha + 1)+ " e coluna " + (coluna + 1) + " : ")
				leia(numeros[coluna][linha])
				linha++	
				se(linha == 2 e coluna != 4)
				{
					escreva("Digite um numero para a linha " + (linha + 1)+ " e coluna " + (coluna + 1) + " : ")
					leia(numeros[coluna][linha])
					linha = 0
					coluna++
				}
			}
		}
		escreva("\n--------------------------\n")
		linha = 0
		coluna = 0
		enquanto(coluna != 4)
		{	
			enquanto (linha < 3)
			{
				se(numeros[coluna][linha] % 2 == 0)
				{
					escreva("\n" + numeros[coluna][linha] + " é par e está na linha " + (linha + 1) + " e na coluna " + (coluna + 1))
				}
				linha++
				se(linha == 2 e coluna != 4)
				{
					se(numeros[coluna][linha] % 2 == 0)
					{
						escreva("\n" + numeros[coluna][linha] + " é par e está na linha " + (linha + 1) + " e na coluna " + (coluna + 1))
					}
					linha = 0
					coluna++
				}
								
			}
	}
}
}

\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
11-
\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\

programa
{
	
	funcao inicio()
	{
		inteiro linha = 0
		inteiro coluna = 0
		logico wally_esta = falso
		cadeia nomes[3][3] = {
			
			{"Walso","Vila","Volley"},
			{"Wilson","Valdo","Wally"},
			{"Wallace","Willy","Vasco"}

		}
			
		enquanto(coluna != 2)
		{	
			enquanto (linha < 3)
			{
				se(wally_esta == falso e nomes[coluna][linha] == "Wally")
				{
					wally_esta = verdadeiro
					escreva("Wally está na matriz na linha " + (linha + 1) + " na coluna " + (coluna + 1))
				}
				linha++	
				se(linha == 2 e coluna != 2)
				{
					se(wally_esta == falso e nomes[coluna][linha] == "Wally")
					{
						wally_esta = verdadeiro
						escreva("Wally está na matriz na linha " + (linha + 1) + " na coluna " + (coluna + 1))
					}
					linha = 0
					coluna++
				}
			}	

		}

		se(wally_esta == falso)
		{
			escreva("Wally não está na matriz")
		}
		
	}
}

