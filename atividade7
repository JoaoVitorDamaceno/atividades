\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\

programa
{
	
	funcao inicio()
	{
		inteiro dia_inserido
		inteiro mes_inserido
		inteiro ult_digito_inserido
		cadeia dia_aniversario[31] = {"O gato ","O guardião ","O babuino ","O pterodátilo ","O idiota ","O piloto ","O papagaio ","O vampiro ","O pedreiro ","O vaqueiro ","O espião ","O guerreiro ","A panqueca ","O humanoide ","O protetor ","O alce ","O robô ","O ganso ","O caixão ","O inseto ","O frango ","O tubarão ","O profeta ","O camelo ","O traficante ","O meteorito ","O esquisito ","O samurai ","O bolinho ","O detetive ","O astronauta "}
		cadeia mes_aniversario[12] = {"Famoso ","Poderoso ","Aterrorizante ","Travesso ","Metafísico ","Roqueiro ","Extravagante ","Aventureiro ","Nuclear ","Demoníaco ","Fantástico ","Sobrenatural "}
		cadeia ult_digito_ano[10] = {"Das salsichas","Da escuridão","Do futuro","Da segurança nacional", "Contra ataca", "Do espaço", "Não conhece piedade", "Do subsolo", "Do abismo", "De marte"}

		escreva("Qual o seu dia de aniversario? : ")
		leia(dia_inserido)

		escreva("Qual o seu mês de nascimento? : ")
		leia(mes_inserido)
		
		escreva("Qual o ultimo digito do ano do seu nascimento? : ")
		leia(ult_digito_inserido)

		escreva("O nome do seu filme seria: " + dia_aniversario[dia_inserido - 1] + mes_aniversario[mes_inserido - 1] + ult_digito_ano[ult_digito_inserido - 1])
		
	}
}

\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
13-
\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\

programa
{

	funcao inicio()
	{
		cadeia convidados[100]
		inteiro opcao = 0
		inteiro pessoas = 0
		cadeia remover
		logico removido = falso
		cadeia espera

		enquanto(opcao >= 0 ou opcao <= 4)
		{

			escreva("\n====================MENU=====================\n")
			escreva("[ 1 ] Cadastrar Pessoa\n")
			escreva("[ 2 ] Listar Pessoas\n")
			escreva("[ 3 ] Remover Pessoa\n")
			escreva("[ 4 ] Sair\n")
			escreva("=========================\n")
			escreva("Opção: ")
			leia(opcao)
			
		enquanto (opcao == 1)
		{
			escreva("Digite o nome da pessoa que gostaria de adicionar a lista: ")
			leia(convidados[pessoas])
			se(convidados[pessoas] == "4")
			{
				opcao = 0
			}
			se(opcao != 0)
			{
				pessoas++
			}
		}

		se (opcao == 2)
		{
			para(inteiro i = 0; i < pessoas; i++)
			{
				escreva((i + 1) +" "+ convidados[i] + "\n")
			}

			escreva("Digite qualquer tecla para voltar para o menu: ")
			leia(espera)
			
			opcao = 0
			
		}

		enquanto(opcao == 3)
		{
			removido = falso
			remover = ""
			escreva("\nQual a pessoa que você gostaria de remover da lista?: ")
			leia(remover)
			se(remover == "4")
			{
				opcao = 0
			}
			se(opcao == 3)
			{
				para(inteiro n = 0; n < pessoas; n++)
				{
					se(remover == convidados[n])
					{
						escreva(remover + " removido da lista!")
						convidados[n] = ""
						removido = verdadeiro
					}

				}
				se (removido == falso)
				{
					escreva("Nome não encontrado")
				}
			}
				
			
		}
		
		
		}
		
		
	}
}
