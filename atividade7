programa
{
	
	funcao inicio()
	{
		real vetor[10] = {1.10,1.11,1.12,1.13,1.14,1.15,1.16,1.17,1.18,1.19} 

		para(inteiro i = 10; i != 0; i--)
		{
			escreva("\n" + vetor[i - 1])
		}
	}
}

