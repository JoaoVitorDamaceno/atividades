5- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

programa
{
	
	funcao inicio()
	{
		inteiro n = 0
		logico n2 = falso
	
		leia(n)
		
		para(inteiro i = 1; i <= n; i++)
		{
			escreva(i)
			se(i == n)
			{
				escreva("\n" + "\n" + "\n")
				n2 = verdadeiro
			}
			
		}

		se(n2)
		{
			para(inteiro i = n; i >= 1; i--)
			{
				escreva(i)
			}
		}
		
	}
}


