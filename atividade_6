1-a) ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

programa
{
	
	funcao inicio()
	{
		
		inteiro n_minimo = 0
		inteiro n_max = 20	
		enquanto(n_minimo < n_max)
		{
			n_minimo++
			escreva("\n"+n_minimo)
	
		}
	}
}

