programa
{
	
	funcao inicio()
	{
		para(inteiro i = 1; i < 20; i++)
		{
			escreva(i + "\n")
		}
	}
}

2- ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

programa
{
	
	funcao inicio()
	{
		
		inteiro n_minimo = 21
		inteiro n_max = 1	
		enquanto(n_minimo > n_max)
		{
			n_minimo--
			escreva("\n")
			escreva(n_minimo)
		}
	}
}

